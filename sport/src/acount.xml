<?xml version="1.0" encoding="UTF-8"?>
<excel id="student" code="student" name="享动平台账单">
    <colgroup>
        <col index="A" width='9em'></col>
        <col index="B" width='10em'></col>
        <col index="C" width='20em'></col>
        <col index="D" width='20em'></col>
        <col index="E" width='20em'></col>
        <col index="F" width='15em'></col>
        <col index="G" width='20em'></col>
        <col index="H" width='15em'></col>
        <col index="I" width='17em'></col>               
    </colgroup>
    <title>
        <tr height="16px">
            <td rowspan="1" colspan="9" value="title" bold="true"  fontsize="20"  />
        </tr>
        <tr height="16px">
            <td rowspan="2" colspan="2" value="已结算金额:" 	bold="false"  fontsize="14" />
            <td rowspan="2" colspan="3" value="payedTotalFee" 	bold="false"  fontsize="14" />
            <td rowspan="2" colspan="4" value="未结算金额" 	bold="false"  fontsize="14" />
            <td rowspan="2" colspan="5" value="totalFee" 	bold="false"  fontsize="14" />
            <td rowspan="2" colspan="6" value="" 	bold="false"  fontsize="14" />
            <td rowspan="2" colspan="7" value="创建日期" 	bold="false"  fontsize="14" />
		    <td rowspan="2" colspan="9" value="createDate" 	bold="false" fontsize="14" />       
        </tr>
    </title>
    <thead>
        <tr height="16px">
        	<th value="编号" />
            <th value="类型" />
            <th value="教练名/公司名" />
            <th value="上一次订单结算时间段" />
            <th value="订单开始时间" />
            <th value="订单结束时间" />
            <th value="可结算金额" />           
            <th value="已结算金额	" />
            <th value="是否已结算" />
        </tr>
    </thead>
    <tbody>
        <tr height="16px" firstrow="2" firstcol="0" repeat="1">
			<td value="@" name="autoNumber"/>
			<td value="@" name="acountType"/>
			<td value="@" name="name"/>
			<td value="#" name="lastestPayedTime"/>
			<td value="@" name="beginDate"/>
			<td value="@" name="endDate"/>
			<td value="#" name="payedTotalFee"/>
			<td value="@" name="totalFee"/>
			<td value="@" name="hasPay"/>
        </tr>
    </tbody>
</excel>